{
  "name": "backbone-forms-bootstrap-typeahead",
  "description": "Backbone-Forms Bootstrap Typeahead editor",
  "dependencies": {},
  "devDependencies": {
    "coffee-script": "^1.10.0",
    "uglify-js": "^2.7.0"
  },
  "scripts": {
    "build": "npm run build-js; npm run uglify",
    "build-js": "coffee --bare --compile --no-header --output dist src/bbf-typeahead.coffee",
    "uglify": "uglifyjs dist/bbf-typeahead.js --output dist/bbf-typeahead.min.js --compress --mangle"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tomi77/backbone-forms-bootstrap-typeahead.git"
  },
  "author": "Tomasz Jakub Rup",
